{
    "title": "Field Access",
    "data": {
        "a": 1,
        "b": {"c" : { "d" : 2 }},
        "e.f": {"g.h" : 3},
        "!@#$+=- %)@!(*%\"": 4,
        ":": 5
    },
    "tests": [{
        "path": "$.a",
        "result": [1]
    }, {
        "path": "$['a']",
        "result": [1]
    },{
        "path": "$[ 'a']",
        "result": [1]
    }, {
        "path": "$['b']['c'].d",
        "result": [2]
    }, {
        "path": "$['e.f']['g.h']",
        "result": [3]
    }, {
        "path": "$['!@#$+=- %)@!(*%\"']",
        "result": [4]
    }, {
        "path": "$[':']",
        "result": [5]
    }]
}
